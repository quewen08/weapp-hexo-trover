{
    "name": "weapp-hexo-trover",
    "version": "1.0.0",
    "private": true,
    "description": "a weapp for hexo blog",
    "templateInfo": {
        "name": "vue3-vuex",
        "typescript": false,
        "css": "sass"
    },
    "scripts": {
        "build:weapp": "taro build --type weapp",
        "build:swan": "taro build --type swan",
        "build:alipay": "taro build --type alipay",
        "build:tt": "taro build --type tt",
        "build:h5": "taro build --type h5",
        "build:rn": "taro build --type rn",
        "build:qq": "taro build --type qq",
        "build:quickapp": "taro build --type quickapp",
        "dev:weapp": "set NODE_ENV=production && taro build --type weapp --watch",
        "dev:swan": "npm run build:swan -- --watch",
        "dev:alipay": "npm run build:alipay -- --watch",
        "dev:tt": "npm run build:tt -- --watch",
        "dev:h5": "npm run build:h5 -- --watch",
        "dev:rn": "npm run build:rn -- --watch",
        "dev:qq": "npm run build:qq -- --watch",
        "dev:quickapp": "npm run build:quickapp -- --watch"
    },
    "browserslist": [
        "last 3 versions",
        "Android >= 4.1",
        "ios >= 8"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.7.7",
        "@tarojs/components": "3.4.3",
        "@tarojs/mini-runner": "^3.4.3",
        "@tarojs/plugin-framework-vue3": "3.4.3",
        "@tarojs/plugin-platform-weapp": "3.4.3",
        "@tarojs/runtime": "3.4.3",
        "@tarojs/taro": "3.4.3",
        "cache-loader": "^4.1.0",
        "taro-plugin-compiler-optimization": "^1.0.1",
        "thread-loader": "^3.0.4",
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.0",
        "@tarojs/webpack-runner": "3.4.3",
        "@types/webpack-env": "^1.13.6",
        "@vue/compiler-sfc": "^3.0.0",
        "babel-plugin-import": "^1.13.3",
        "babel-preset-taro": "3.4.3",
        "eslint": "^6.8.0",
        "eslint-config-taro": "3.4.3",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^7.0.0",
        "stylelint": "^9.3.0",
        "terser-webpack-plugin": "^3.0.5",
        "vue-loader": "^16.0.0-beta.8"
    }
}
